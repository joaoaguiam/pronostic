pragma solidity ^0.4.22;

import "truffle/Assert.sol";
import "truffle/DeployedAddresses.sol";
import "../contracts/WCwagers.sol";

contract TestWCwagers {

  function testParticipantCreation() public {
    WCwagers contest = WCwagers(DeployedAddresses.WCwagers());

    contest.registerParticipant();
    address expected = address(this);
    string[] participants = contest.getParticipants();
    Assert.equal(participants[0], expected, "first participant is this test account");
  }
